#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/bin/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/bin/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules:/mnt/d/Radha - Learning/HTML/Mulampuzha Library/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/bin/node-pg-migrate.js" "$@"
else
  exec node  "$basedir/../.pnpm/node-pg-migrate@8.0.3_pg@8.16.3/node_modules/node-pg-migrate/bin/node-pg-migrate.js" "$@"
fi
